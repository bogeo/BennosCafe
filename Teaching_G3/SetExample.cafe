-- Lehrveranstaltung "Algorithmen & Datenstrukturen"
-- Aufgabe 3.1-5: ADT Menge von Items des Typs T

input Set .
open Set .
  vars a b c : T
  red isEmpty(insert(a, empty)) .   
  red isIn(a, insert(a, empty)) .
  let mySet = insert(a, insert(b, insert(a, empty))) .
  red isIn(a, mySet) .
  red isIn(b, mySet) .
  red isIn(c, mySet) .
close .

 